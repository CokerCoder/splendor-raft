    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch ClientLauncher",
            "request": "launch",
            "args": "-gc A,localhost,3333 B,localhost,3334 C,localhost,3335",
            "mainClass": "com.da.kv.client.ClientLauncher",
            "projectName": "splendor-raft"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "mainClass": "com.da.App",
            "projectName": "splendor-raft"
        },
        {
            "type": "java",
            "name": "Launch ServerLauncher",
            "request": "launch",
            "args": "-gc A,localhost,2333 B,localhost,2334 C,localhost,2335 -m group-member -i A -p2 3333",
            "mainClass": "com.da.kv.server.ServerLauncher",
            "projectName": "splendor-raft"
        }
    ]
}